<div class="container">
      <!-- Search -->
      <div id="search-place-details-row" class="top-search row">
        <div class="col-xs-12 col-md-12">
          <form class="" role="search">
            <!-- <div class="input-group"> -->
              <input id="search-place-details-input" type="text" data-provide="typeahead" autocomplete="off" class="form-control" placeholder="Learn more about . . ."/>
              <input id="search-place-details-submit" type="submit" style="position: absolute; left: -9999px; width: 1px; height: 1px;"/>
<!--               <div class="input-group-btn">
                <button id="search-place-details-enter" type="submit" class="btn btn-default">
                  <span class="glyphicon glyphicon-search">
                    <span class="sr-only">Search...</span>
                  </span>
                </button>
              </div> -->
            <!-- </div> -->
          </form>
        </div>
      </div>

      <!-- Place not found -->
      <div id="place-details-not-found">Place not found.</div>

      <!-- Place details -->
      <div id="place-details" class="row" style="display:none">

          <!-- Info -->
          <div class="place-details-inner col-xs-12 col-md-7">
            <h3 id="place-details-name"></h3>
            <div id="hours-day-labels" class="col-xs-1 col-md-1">
              <h6>M</h6>
              <h6>T</h6>
              <h6>W</h6>
              <h6>Th</h6>
              <h6>F</h6>
              <h6>S</h6>
              <h6>Su</h6>
            </div>
            <div id="hours-times" class="col-xs-5 col-md-5">
              <h6 id="hours-times-m"></h6>
              <h6 id="hours-times-t"></h6>
              <h6 id="hours-times-w"></h6>
              <h6 id="hours-times-th"></h6>
              <h6 id="hours-times-f"></h6>
              <h6 id="hours-times-s"></h6>
              <h6 id="hours-times-su"></h6>
            </div>
            <div id="amenity-labels" class="col-xs-3 col-md-2">
              <h6>Wi-Fi</h6>
              <h6>Outlets</h6>
              <!-- <h6>Bathrooms</h6> -->
              <h6>Coffee</h6>
              <h6>Food</h6>
            </div>
            <div id="amenity-val" class="col-xs-2 col-md-2">
              <h6 id="amenity-val-wifi"></h6>
              <h6 id="amenity-val-outlets"></h6>
              <!-- <h6 id="amenity-val-bathrooms"></h6> -->
              <h6 id="amenity-val-coffee"></h6>
              <h6 id="amenity-val-food"></h6>
            </div>
          </div>

          <!-- Map -->
          <div id="place-details-map" class="place-details-inner col-xs-12 col-md-5">
              <iframe id="place-details-map-iframe" width="100%" height="300" frameborder="0" style="border:0"></iframe>
          </div>

      </div>
      <!-- End of place details -->
</div>

<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&signed_in=true&libraries=places"></script>
<script src="/js/typeahead-bundle.js"></script>
<script src="/js/searchPlaceDetails.js"></script>