<div class="container">

      <!-- Search -->
      <div id="search-places-row" class="top-search row">
        <div class="col-xs-12">
          <form class="" role="search">
            <div class="input-group">
              <input id="search-places-input" type="text" class="form-control" placeholder="Find a study spot near. . ." disabled>
              <div class="input-group-btn">
                <button id="search-places-enter" type="submit" class="btn btn-default">
                  <span class="glyphicon glyphicon-search">
                    <span class="sr-only">Search...</span>
                  </span>
                </button>
              </div>
            </div>
          </form>
        </div>
      </div>

      <!-- Map -->
      <div id="map-canvas" class="col-xs-12 col-md-12"></div>

      <!-- Modal -->
      <div class="modal fade" id="place-detail-overlay" role="dialog">
        <div class="modal-dialog">
          <div class="modal-content">
              <div class="modal-body">

                  <!-- Place details -->
                  <div id="place-details-overlay" class="row" style="display:none">

                      <!-- Info -->
                      <div class="place-details-inner col-xs-12 col-md-8">
                        <h3 id="place-details-name-overlay"></h3>
                        <div id="hours-day-labels-overlay" class="col-xs-1 col-md-1">
                          <h6>M</h6>
                          <h6>T</h6>
                          <h6>W</h6>
                          <h6>Th</h6>
                          <h6>F</h6>
                          <h6>S</h6>
                          <h6>Su</h6>
                        </div>
                        <div id="hours-times-overlay" class="col-xs-5 col-md-6">
                          <h6 id="hours-times-m-overlay"></h6>
                          <h6 id="hours-times-t-overlay"></h6>
                          <h6 id="hours-times-w-overlay"></h6>
                          <h6 id="hours-times-th-overlay"></h6>
                          <h6 id="hours-times-f-overlay"></h6>
                          <h6 id="hours-times-s-overlay"></h6>
                          <h6 id="hours-times-su-overlay"></h6>
                        </div>
                        <div id="amenity-labels-overlay" class="col-xs-3 col-md-3">
                          <h6>Wi-Fi</h6>
                          <h6>Outlets</h6>
                          <h6>Coffee</h6>
                          <h6>Food</h6>
                        </div>
                        <div id="amenity-val-overlay" class="col-xs-1 col-md-1">
                          <h6 id="amenity-val-wifi-overlay"></h6>
                          <h6 id="amenity-val-outlets-overlay"></h6>
                          <h6 id="amenity-val-coffee-overlay"></h6>
                          <h6 id="amenity-val-food-overlay"></h6>
                        </div>
                      </div>

                      <!-- Map -->
                      <div id="place-details-map-overlay" class="place-details-inner col-xs-12 col-md-4">
                          <iframe id="place-details-map-iframe-overlay" width="100%" height="200" frameborder="0" style="border:0" src="https://www.google.com/maps/embed/v1/place?q=Geisel%20Library%2C%20Gilman&key=AIzaSyDQRjMnj-tHPC2FnAE8xhQ-HyoiUHeYQdQ"></iframe>
                      </div>

                  </div>
                  <!-- End of place details -->

              </div>
          </div>
        </div>
      </div>
</div>

<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&signed_in=true&libraries=places"></script>
<script src="/js/nearby-places.js"></script>