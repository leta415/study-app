<!-- Menu items -->
<div class="container-fluid">
  <div class="menu row-fluid">
    {{#each menu-items}}
    <div class="col-xs-4 menu-item">
      <a href="../{{page}}/">
        <img src="/img/{{img}}" class="icon img-responsive" />
        <p>{{name}}</p>
      </a>
    </div>
    {{/each}}
  </div>
</div> 